{
  "name": "uni-room-finder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "@types/react": "^16.8.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^3.0.11",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.4.1",
    "react-select": "^2.4.1",
    "typescript": "^3.3.3333",
    "vsts-coverage-styles": "^1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ci": "react-scripts test --env=jsdom --reporters=default --reporters=jest-junit --coverage",
    "posttest": "node ./tools/inline-code-coverage.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/react-dom": "^16.8.2",
    "firebase": "7.14.0",
    "jest-junit": "^6.3.0",
    "prettier": "1.16.4"
  },
  "jest": {
    "coverageReporters": [
      "cobertura",
      "html"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
